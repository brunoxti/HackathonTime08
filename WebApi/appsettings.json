{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "ZabbixApi": {
    "BaseUrl": "https://opti.xpinc.io/zabbix-hml",
    "ActionMethod": {
      "create": "action.create",
      "delete": "action.delete",
      "update": "action.update",
      "get": "action.get"
    },
    "AlertMethod": {
      "get": "alert.get"
    },
    "EventMethod": {
      "get": "event.get",
      "ack": "event.acknowledge"
    },
    "ProblemMethod": {
      "get": "problem.get"
    },
    "LoginMethod": {
      "login": "user.login"
    }
  } 
}
